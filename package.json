{
  "name": "esm-bench",
  "version": "1.0.0",
  "description": "",
  "main": "bench.js",
  "scripts": {
    "build": "npm run build-app && npm run build-lodash-cjs && npm run build-lodash-esm",
    "build-app": "rollup -c",
    "build-lodash-esm": "rollup ./node_modules/lodash-es/lodash.js --file ./bench/lodash.esm.js --format esm && rollup ./node_modules/lodash-es/lodash.js --file ./bench/lodash.esm.min.js --format esm --compact",
    "build-lodash-cjs": "rollup ./node_modules/lodash/lodash.js --file ./bench/lodash.js --format cjs && rollup ./node_modules/lodash/lodash.js --file ./bench/lodash.min.js --format cjs --compact",
    "lodash-cjs": "lodash -o ./lodash-cjs",
    "lodash-esm": "lodash modularize exports=es -o ./lodash-esm",
    "postinstall": "npm run build",
    "start": "node ./src/server.js",
    "watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asilvas/esm-bench.git"
  },
  "author": "Aaron Silvas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asilvas/esm-bench/issues"
  },
  "homepage": "https://github.com/asilvas/esm-bench#readme",
  "devDependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.11",
    "lodash-cli": "^4.17.5",
    "lodash-es": "^4.17.11",
    "p-limit": "^2.2.0",
    "rollup": "^1.16.2",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.1.0",
    "shimport": "^1.0.1"
  }
}
